URxvt.perl-ext-common:	default,clipboard,url-select,keyboard-select
URxvt.per-select.launcher:	chromium
URxvt.url-select.underline:	ture
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.keysym.M-Escape:	perl:keyboard-select:activate
URxvt.keysym.M-s:	perl:keyboard-select:search

URxvt*transparent: true
URxvt*saveLines: 12000
URxvt*font: xft:DejaVu Sans Mono:pixelsize=12:antialias=false:hinting=true
URxvt*boldFont: xft:DejaVu Sans Mono:bold:pixelsize=12:antialias=false:hinting=true
URxvt*scrollBar: false
URxvt*scrollBar_right: true
URxvt*scrollstyle: rxvt

URxvt*foreground: #17B2FF
URxvt*background: #000000

! black
URxvt*color0  : #2E3436
URxvt*color8  : #555753
! red
URxvt*color1  : #a31604
URxvt*color9  : #c60001
! green
URxvt*color2:    #06989A
URxvt*color10:   #76550F
! yellow
URxvt*color3  : #c1951a
URxvt*color11 : #d5a30e
! blue
URxvt*color4  : #425387
URxvt*color12 : #4a5a8d
! magenta
URxvt*color5  : #965d98
URxvt*color13 : #893c8c
! cyan
URxvt*color6  : #06989A
URxvt*color14 : #12bccb
! white
URxvt*color7  : #D3D7CF
URxvt*color15 : #EEEEEC
